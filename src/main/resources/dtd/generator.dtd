    <!ELEMENT jdbc.driver (#PCDATA)>
    <!ELEMENT jdbc.url (#PCDATA)>
    <!ELEMENT jdbc.username (#PCDATA)>
    <!ELEMENT jdbc.password (#PCDATA)>
    <!ELEMENT parent.package (#PCDATA)>
    <!ELEMENT dao.package (#PCDATA)>
    <!ELEMENT service.package (#PCDATA)>
    <!ELEMENT controller.package (#PCDATA)>
    <!ELEMENT module.path (#PCDATA)>
    <!ELEMENT xml.path (#PCDATA)>
    <!ELEMENT base-model (#PCDATA)>
    <!ELEMENT ignore-column (#PCDATA)>
    <!ELEMENT id-util (#PCDATA)>
    <!ELEMENT result-model (#PCDATA)>
